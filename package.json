    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass-migrator": "^1.5.2",
    "smart-grid": "^2.1.2",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "webpack-config-create-utils": "^1.0.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "assets-webpack-plugin": "^7.1.1",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "nodemon": "^2.0.16",
    "react-redux": "^8.0.2",
    "react-router-dom": "^5.3.1",
