{
  "name": "webpack-config-layout",
  "version": "1.0.0",
  "description": "default webpack layout",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack server --mode development --open --hot",
    "server:build": "cross-env NODE_ENV=development webpack --mode development",
    "server:start": "cross-env NODE_ENV=development nodemon server.js",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "build:grid": "node smart-grid.js && sass-migrator division src/**/*.scss"
  },
